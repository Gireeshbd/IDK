{
  "name": "smartcon",
  "version": "0.1.0",
  "scripts": {
    "build-contract": "solc contracts/Gravity.sol --abi -o abis --overwrite && solc contracts/Gravity.sol --bin -o bin --overwrite",
    "create": "graph create Gireeshbd/smartcon --node https://api.thegraph.com/deploy/",
    "create-local": "graph create Gireeshbd/smartcon --node http://127.0.0.1:8020",
    "codegen": "graph codegen",
    "build": "graph build",
    "deploy": "graph deploy Gireeshbd/smartcon --ipfs https://api.thegraph.com/ipfs/ --node https://api.thegraph.com/deploy/",
    "deploy-local": "graph deploy Gireeshbd/smartcon --ipfs http://127.0.0.1:5001 --node http://127.0.0.1:8020"
  },
  "devDependencies": {
    "@graphprotocol/graph-cli": "^0.28.1",
    "@graphprotocol/graph-ts": "^0.26.0"
  },
  "dependencies": {
    "@openzeppelin/subgraphs": "^0.1.8",
    "babel-polyfill": "^6.26.0",
    "babel-register": "^6.26.0",
    "truffle": "^5.0.4",
    "truffle-contract": "^4.0.5",
    "truffle-hdwallet-provider": "^1.0.4"
  },
  "main": "index.js",
  "author": "Gireesh <gireeshreddy1945@gmail.com>",
  "license": "MIT"
}
